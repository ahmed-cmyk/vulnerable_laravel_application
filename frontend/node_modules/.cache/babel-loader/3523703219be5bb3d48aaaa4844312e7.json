state.user.user;\n    },\n  },\n  async created() {\n    const check = await this.$store.dispatch(\"checkLocalStorage\");\n\n    if (check) {\n      this.$store.dispatch(\"autoLogin\");\n    }\n  },\n  methods: {\n    logout() {\n      window.localStorage.removeItem(\"authToken\");\n      this.$store.dispatch(\"deleteToken\");\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}